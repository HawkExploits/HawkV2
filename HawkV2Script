UICorner_33.Parent = FoodShop
local Plr = game.Players.LocalPlayer
AntiSlow.MouseButton1Click:Connect(function()
	coroutine.resume(coroutine.create(function()


		while true do
			pcall(function()
				if game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("ReduceWalk") then
					game.Players.LocalPlayer.Character.BodyEffects.Movement.ReduceWalk:Destroy()
				end

				if game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("NoJumping") then
					game.Players.LocalPlayer.Character.BodyEffects.Movement.NoJumping:Destroy()
				end
				if game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("NoWalkSpeed") then
					game.Players.LocalPlayer.Character.BodyEffects.Movement.NoWalkSpeed:Destroy()
				end
			end)
			wait()
		end

	end))

end)



-- Anti Cheat Bypas --


--// AntiCheat --//
coroutine.resume(coroutine.create(function()
	while true do
		pcall(function()
			for i,v in pairs(Plr.Character:GetChildren()) do
				if v:IsA("Script") then
					if v.Name == "Animate" then return end
					if v.Name == "Health" then return end
					v:Destroy()
				end
			end
		end)
		wait()
	end
end))
--// Page2 --//

Page2Button.MouseButton1Click:Connect(function()
	Page2.Visible = true
	Page1.Visible = false
	game.StarterGui:SetCore("SendNotification", {
		Title = "HachioV2",
		Text = "Now opened Page 2",
		Duration = "3",
	})
end)

--// Page1 --//
Page1Button.MouseButton1Click:Connect(function()
	Page1.Visible = true
	Page2.Visible = false
	game.StarterGui:SetCore("SendNotification", {
		Title = "HachioV2",
		Text = "Now opened Page 1",
		Duration = "3",
	})
end)
--// AntiStomp --//
local AntiStomp = true
Antistomp.MouseButton1Click:Connect(function()
	game.StarterGui:SetCore("SendNotification", {
		Title = "HachioV2",
		Text = "Now enabled Anti Stomp",
		Duration = "3",
	})
	Antistomp = true
	coroutine.resume(coroutine.create(function()
		while Antistomp == true do
			pcall(function()
				game.Players.LocalPlayer.Character.BodyEffects["K.O"]:GetPropertyChangedSignal("Value"):Connect(function()
					if game.Players.LocalPlayer.Character.BodyEffects["K.O"].Value == true then
						game.Players.LocalPlayer.Character:Destroy()
					end
				end)
			end)
			wait()
		end
	end))
end)

--// GodBullet --//
local Plr = game.Players.LocalPlayer
GodBullet.MouseButton1Click:Connect(function()
	if Plr.Character.BodyEffects:FindFirstChild("Armor") then
		Plr.Character.Armor:Destroy()
	end	
end)

-- Godmode --//
local GodMode = false
Godmode.MouseButton1Click:Connect(function()
	if GodMode == false then
		game.StarterGui:SetCore("SendNotification", {
			Title = "HachioV2",
			Text = "Now godded.",
			Duration = "7",
		})
		local player = game.Players.LocalPlayer
		player.Character:Destroy()
		GodMode = true
		player.CharacterAdded:Connect(function(char)
			if GodMode == true then
				local Folder = Instance.new("Folder",char)
				Folder.Name = "FULLY_LOADED_CHAR"
			end
		end)
		while GodMode == true do
			game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack,true)
			for i,v in pairs(player.Character:GetDescendants()) do
				if v.Name == "BreakingParts" then
					v:Destroy()
				end
			end
			wait()
		end
	else
		game.StarterGui:SetCore("SendNotification", {
			Title = "HachioV2",
			Text = "Now disabled god mode.",
			Duration = "7",
		})
		Plr.Character:Destroy()
		GodMode = false
	end
end)

--// Noclip --//


Noclip.MouseButton1Click:Connect(function()

	game.StarterGui:SetCore("SendNotification", {
		Title = "HachioV1",
		Text = "R to noclip/unclip",
		Duration = "7",
	})
	noclip = false
	game:GetService('RunService').Stepped:connect(function()
		if noclip then
			game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
		end
	end)
	local plr = game.Players.LocalPlayer
	local mouse = plr:GetMouse()
	mouse.KeyDown:connect(function(key)

		if key == "r" then
			noclip = not noclip
			game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
		end
	end)
end)

--// Reach --//

local ReachON = false
Reach.MouseButton1Click:Connect(function()
	if not ReachON then
		game.StarterGui:SetCore("SendNotification", {
			Title = "HachioV1",
			Text = "Reaching is on click again to disable!",
			Duration = "7",
		})
		for _, d in next, workspace:GetDescendants() do
			if (d:IsA'Seat' or d:IsA'VehicleSeat') then
				d:Remove();
			end;
		end;
		coroutine.resume(coroutine.create(function()
			while true do
				for _, d in next, workspace:GetDescendants() do
					if (d:IsA'Seat' or d:IsA'VehicleSeat') then
						d:Remove();
					end;
				end;
				wait(10)
			end
		end))
		ReachON = true
		game.Players.LocalPlayer.Character.RightHand.Size = Vector3.new(25, 25, 25)
		game.Players.LocalPlayer.Character.LeftHand.Size = Vector3.new(25, 25, 25)
		game.Players.LocalPlayer.Character.RightHand.Massless = true
		game.Players.LocalPlayer.Character.LeftHand.Massless = true
		game.Players.LocalPlayer.Character.RightHand.Transparency = 1
		game.Players.LocalPlayer.Character.LeftHand.Transparency = 1 
	else
		ReachON = false
		game.StarterGui:SetCore("SendNotification", {
			Title = "HachioV2",
			Text = "Reaching is off click again to enable!",
			Duration = "7",
		})
		game.Players.LocalPlayer.Character.RightHand.Size = Vector3.new(0.5, 0.5, 0.5)
		game.Players.LocalPlayer.Character.LeftHand.Size = Vector3.new(0.5, 0.5, 0.5)
		game.Players.LocalPlayer.Character.RightHand.Massless = true
		game.Players.LocalPlayer.Character.LeftHand.Massless = true
		game.Players.LocalPlayer.Character.RightHand.Transparency = 0
		game.Players.LocalPlayer.Character.LeftHand.Transparency = 0   
	end
end)



